    "micro services",
    "micro service",
    "networking",
    "distributed",
    "distributed-message",
    "distributed message",
    "loadbalancing",
    "loadbalance"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --check-coverage mocha --exit --timeout 10000 ./test/*.js",
    "snyktest": "snyk test",
    "standard": "standard './src/**/*.js' --parser babel-eslint --verbose | snazzy",
    "format": "standard './src/**/*.js' --parser babel-eslint  --fix --verbose | snazzy",
    "rimraf": "rimraf",
    "clear": "rimraf ./dist",
    "compile": "./node_modules/babel-cli/bin/babel.js -d dist/ src/",
    "prepublish": "npm run build",
    "build": "npm run clear && npm run compile"
  },
  "bin": {
    "kitoo-core": "./bin/kitoo-core.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfast/kitoo-core.git"
  },
  "author": "Steadfast Teach - http://steadfast.tech",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sfast/kitoo-core/issues"
  },
  "homepage": "https://github.com/sfast/kitoo-core#readme",
  "dependencies": {
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.7",
    "fs-extra": "^3.0.1",
    "lokijs": "^1.4.3",
    "read-config": "^2.0.0",
    "semver": "^5.5.0",
    "shortid": "^2.2.8",
    "underscore": "latest",
    "uuid": "^3.1.0",
    "vorpal": "^1.12.0",
    "yargs": "^12.0.2",
    "zeronode": "^1.1.24"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-helper": "0.0.6",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "chai": "^4.1.0",
    "cross-env": "^5.1.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "mocha": "^5.0.5",
    "nyc": "^11.4.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "webpack": "^2.5.1"
  },
  "snyk": false,
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
